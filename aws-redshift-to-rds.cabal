name:             aws-redshift-to-rds
version:          0.1.0
author:           Dat Le
category:         Database
build-type:       Simple
cabal-version:    >= 1.10

executable aws-redshift-to-rds
  default-language:
    Haskell2010
  hs-source-dirs:
    src
  main-is:
    Main.hs
  build-depends:
    base == 4.*,
    postgresql-simple,
    safe,
    bytestring,
    optparse-applicative==0.9.*,
    string-conversions,
    interpolate,
    configurator
  ghc-options:
    -Wall -Werror -fno-warn-name-shadowing
    -threaded
